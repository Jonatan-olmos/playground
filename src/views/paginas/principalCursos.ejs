<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head.ejs',{title : "inicio" }) %>

<body>
  <%- include('../partials/header.ejs',) %>
    <main class="main_principalCursos">


      <nav class="nav_principalCursos " style="--bs-breadcrumb-divider: '>';  " aria-label="breadcrumb">
        <ol class="breadcrumb  ">
          <li class="breadcrumb-item  "><a href="/inicio" class="main_principalCursos_a">Mis cursos </a></li>
          <li class="breadcrumb-item active" aria-current="page"><a href="#" class="main_principalCursos_b">Formate
              2024</a> </li>
        </ol>
      </nav>


      <section class="section_principalCursos">
        <section class="section_principalCursos-section">
          <h2 class="section_principalCursos-section-h2">Continúa cursando</h2>

          <article class="main_principalCursos_articulo">

            <div class="main_principalCursos_articulo_div">

              <div class="main_principalCursos_articulo_div1">
                <i class="fa-solid fa-play" style="color:  #375ecf ; "></i>
              </div>
              <div class="main_principalCursos_articulo_div2"></div>

            </div>
            <div class="div_principalCursos_articulo-1">
              <h3 class="main_principalCursos_articulo_h3">Formate 2024</h3>

              <h2 class="main_principalCursos_articulo_h2">titulo: sub titulo</h2>
              <div class="div_principalCursos_titulo-modulo">


                <p class="main_principalCursos_articulo_p">modulo x</p>
                <p class="main_principalCursos_articulo_x">classe: x - Episodio: x</p>
              </div>
            </div>

          </article>
        </section>
        <section class="section_principalCursos-section-1">
          <h3 class="main_principalCursos_izquierda_p">Curso en Empresa</h3>
          <h2 class="main_principalCursos_izquierda_h2">Formacion 2024 - 021303 </h2>

          <p class="main_principalCursos_izquierda_p">Tu progreso</p>
     <article class="article-principalCursos_articulo">

      <div class="div-principalCursos_articulo">

        <%- include('../partials/cursoInContenido.ejs',) %>
        <div class="div3-principalCursos_articulo">
        <p class="div3-principalCursos_articulo-p">40%</p>
        <p  class="div3-principalCursos_articulo-p2">Contenido</p>
      </div>
      </div>
      
      <div class="div-principalCursos_articulo">
      
        <%- include('../partials/cursoInModulo.ejs',) %>
        <div class="div3-principalCursos_articulo">
        <p class="div3-principalCursos_articulo-p">9/10</p>
        <p class="div3-principalCursos_articulo-p2">Modulo</p>
      </div>
      </div>
     </article>   




          </article>
        </section>
      </section>

      <section>

        <h2 class="section_principalCursos-section-h2">Contenido</h2>
        <section class="section_principalCursos-section-3">


          <div class="section_principalCursos-section-5">
            <div class="section_principalCursos-section-2">
              <%- include('../partials/cursoInNumeros.ejs',) %>
            </div>
            <div class="section_principalCursos-section-4">
              <%- include('../partials/cursoInNumeros.ejs',) %>
            </div>
          </div>

          <div class="section_principalCursos-section-7">
            <div class="section_principalCursos-section-6">
              <p>Módulo 1</p>
              <h4>Introducción</h4>
              <p>5 clases</p>



              <div class="accordion">
               
           

                <% cursos.forEach(curso => { %>
                  <%- include('../partials/TitulosCurso', {principal: curso.principal, titulo: curso.titulo,id: curso.id, temass: curso.temass,clipboard:curso.clipboard, clipboard_2: curso.clipboard_2, video: curso.video, video2: curso.video2, clipboard_3: curso.clipboard_3,square : curso.square }) %>
              <% }); %>
            
              </div>


            </div>
          </div>

        </section>
      </section>



    </main>
    <%- include('../partials/scripts.ejs') %>

</body>

</html>